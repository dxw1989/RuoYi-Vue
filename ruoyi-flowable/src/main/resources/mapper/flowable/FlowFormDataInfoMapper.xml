<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ruoyi.flowable.mapper.FlowFormDataInfoMapper">

    <resultMap id="FlowFormDataInfoResult" type="com.ruoyi.flowable.domain.FlowFormDataInfo">
        <id     column="data_id"             property="dataId" />
        <result column="form_id"             property="formId" />
        <result column="form_key"            property="formKey" />
        <result column="business_key"        property="businessKey" />
        <result column="process_instance_id" property="processInstanceId" />
        <result column="task_id"             property="taskId" />
        <result column="task_key"            property="taskKey" />
        <result column="form_values"         property="formValues" />
        <result column="submit_by"           property="submitBy" />
        <result column="submit_time"         property="submitTime" />
        <result column="create_by"           property="createBy" />
        <result column="create_time"         property="createTime" />
        <result column="update_by"           property="updateBy" />
        <result column="update_time"         property="updateTime" />
        <result column="remark"              property="remark" />
        <result column="del_flag"            property="delFlag" />
    </resultMap>

    <sql id="Base_Column_List">
        data_id, form_id, form_key, business_key, process_instance_id, task_id, task_key,
        form_values, submit_by, submit_time, create_by, create_time, update_by, update_time, remark, del_flag
    </sql>

    <select id="selectFlowFormDataInfoById" parameterType="Long" resultMap="FlowFormDataInfoResult">
        SELECT <include refid="Base_Column_List" />
        FROM tbl_form_form_data_info
        WHERE data_id = #{dataId}
    </select>

    <select id="selectFlowFormDataInfoList" parameterType="com.ruoyi.flowable.domain.FlowFormDataInfo" resultMap="FlowFormDataInfoResult">
        SELECT <include refid="Base_Column_List" />
        FROM tbl_form_form_data_info
        <where>
            <if test="formId != null">
                AND form_id = #{formId}
            </if>
            <if test="formKey != null and formKey != ''">
                AND form_key = #{formKey}
            </if>
            <if test="processInstanceId != null and processInstanceId != ''">
                AND process_instance_id = #{processInstanceId}
            </if>
            <if test="taskId != null and taskId != ''">
                AND task_id = #{taskId}
            </if>
            <if test="businessKey != null and businessKey != ''">
                AND business_key = #{businessKey}
            </if>
            <if test="submitBy != null and submitBy != ''">
                AND submit_by = #{submitBy}
            </if>
            <if test="delFlag != null and delFlag != ''">
                AND del_flag = #{delFlag}
            </if>
        </where>
        ORDER BY submit_time DESC
    </select>

    <select id="selectLatestByInstanceId" parameterType="String" resultMap="FlowFormDataInfoResult">
        SELECT <include refid="Base_Column_List" />
        FROM tbl_form_form_data_info
        WHERE process_instance_id = #{processInstanceId}
        ORDER BY submit_time DESC, data_id DESC
        LIMIT 1
    </select>

    <insert id="insertFlowFormDataInfo" parameterType="com.ruoyi.flowable.domain.FlowFormDataInfo" useGeneratedKeys="true" keyProperty="dataId">
        INSERT INTO tbl_form_form_data_info (
            form_id, form_key, business_key, process_instance_id, task_id, task_key,
            form_values, submit_by, submit_time, create_by, create_time, update_by, update_time, remark, del_flag
        ) VALUES (
            #{formId}, #{formKey}, #{businessKey}, #{processInstanceId}, #{taskId}, #{taskKey},
            #{formValues}, #{submitBy}, #{submitTime}, #{createBy}, #{createTime}, #{updateBy}, #{updateTime}, #{remark}, #{delFlag}
        )
    </insert>

    <update id="updateFlowFormDataInfo" parameterType="com.ruoyi.flowable.domain.FlowFormDataInfo">
        UPDATE tbl_form_form_data_info
        <set>
            <if test="formId != null">form_id = #{formId},</if>
            <if test="formKey != null">form_key = #{formKey},</if>
            <if test="businessKey != null">business_key = #{businessKey},</if>
            <if test="processInstanceId != null">process_instance_id = #{processInstanceId},</if>
            <if test="taskId != null">task_id = #{taskId},</if>
            <if test="taskKey != null">task_key = #{taskKey},</if>
            <if test="formValues != null">form_values = #{formValues},</if>
            <if test="submitBy != null">submit_by = #{submitBy},</if>
            <if test="submitTime != null">submit_time = #{submitTime},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
        </set>
        WHERE data_id = #{dataId}
    </update>

    <delete id="deleteFlowFormDataInfoByIds" parameterType="Long">
        DELETE FROM tbl_form_form_data_info WHERE data_id IN
        <foreach collection="array" item="dataId" open="(" close=")" separator=",">
            #{dataId}
        </foreach>
    </delete>

    <delete id="deleteFlowFormDataInfoById" parameterType="Long">
        DELETE FROM tbl_form_form_data_info WHERE data_id = #{dataId}
    </delete>
</mapper>
